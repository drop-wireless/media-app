{
  "name": "dropmedia",
  "version": "1.4",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "buildDebug": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "assembleRelease": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && find ./android/app/src/main/res/drawable-* -type f ! -name 'displayicon.png' ! -name 'bluedot.png' ! -name 'launch_screen.png' -delete && rm -rf ./android/app/src/main/res/raw && cd android && ./gradlew app:assembleRelease",
    "bundleRelease": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && find ./android/app/src/main/res/drawable-* -type f ! -name 'displayicon.png' ! -name 'bluedot.png' ! -name 'launch_screen.png' -delete && rm -rf ./android/app/src/main/res/raw && cd android && ./gradlew bundleRelease"
  },
